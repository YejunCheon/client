# Development Environment (Mock API)
# 개발 환경 - MSW를 사용한 Mock API (백엔드 연결 없이 프론트엔드에서 fake data 반환)

# API Mode: 'mock' | 'http'
# mock: Mock API 사용 (백엔드 불필요, 프론트엔드에서 fake data 반환)
# http: 실제 백엔드 서버 연결 (HTTP 요청)
NEXT_PUBLIC_API_MODE=mock

# API URL (mock 모드에서는 사용하지 않음, http 모드로 전환 시 사용)
NEXT_PUBLIC_API_URL=

# WebSocket URL (mock 모드에서는 사용하지 않음)
NEXT_PUBLIC_WS_URL=

# 환경
NEXT_PUBLIC_ENV=development

# 본인인증 URL (Mock)
NEXT_PUBLIC_VERIFY_START_URL=http://mockapi-env.eba-mpd9wfbe.ap-northeast-2.elasticbeanstalk.com/mock-verify/start

# 본인인증 토큰 TTL (초)
NEXT_PUBLIC_VERIFY_TOKEN_TTL_SECONDS=60
